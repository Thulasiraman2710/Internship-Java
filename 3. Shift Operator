public class Main {  
    public static void main(String[] args)  
    {  
  System.out.println(24<<5); 
  System.out.println(27<<103);
  System.out.println(18>>4);
  System.out.println(30>>3);
  System.out.println(16>>1);
  System.out.println(16>>>1);
  System.out.println(-16>>1);
  System.out.println(-16>>>1);
    }
}
