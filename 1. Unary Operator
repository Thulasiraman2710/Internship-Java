public class Main {
    public static void main (String args[]) {
        int x =+27;
        System.out.println(x);
        System.out.println(-x);
        System.out.println(x++);
        System.out.println(++x);
        System.out.println(x--);
        System.out.println(--x);
        int a =16;
        int b =-28;
        boolean c =true;
        boolean d =false;
        System.out.println(~a);
        System.out.println(~b);
        System.out.println(!c);
        System.out.println(!d);
    }
}
